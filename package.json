    "pack": "ncc build src/main.ts",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:watchAll": "npm run test -- --watchAll",
    "test:coverage": "npm run test -- --coverage",
    "test:coverage:watch": "npm run test -- --coverage --watch",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"**/*.ts\" \"**/*.js\" \"**/*.yml\"",
    "format:check": "prettier --check \"src/**/*.ts\" jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JosephDuffy/xcode-transition-action.git"

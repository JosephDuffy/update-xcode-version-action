  "license": "MIT",
  "dependencies": {
    "@actions/cache": "^0.2.1",
    "@actions/core": "^1.2.4",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^3.0.0",
    "@actions/tool-cache": "^1.5.5",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.11",
    "@typescript-eslint/parser": "^3.1.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.1.0",
    "eslint-plugin-github": "^4.0.1",
    "eslint-plugin-jest": "^23.13.2",
    "jest": "^26.0.1",
    "jest-circus": "^26.0.1",
    "js-yaml": "^3.14.0",
    "prettier": "^2.0.5",
